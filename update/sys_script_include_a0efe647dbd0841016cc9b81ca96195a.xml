        <name>GitLabUtils</name>
        <script><![CDATA[var GitLabUtils = Class.create();
GitLabUtils.prototype = {
	
    initialize: function() { },
	
	postProcess_ListUserProjects: function(inputs, outputs) {
		outputs.status = inputs.statusCode;
		if (inputs.statusCode == '200') {
			outputs.projects = inputs.responseBody;
		} else {
			outputs.error_message = JSON.parse(inputs.responseBody).message;
		}
		return outputs;
	},

    type: 'GitLabUtils'
};]]></script>
